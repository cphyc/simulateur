main() = o:[s]
where
  state = scheduler();
  write_ram = 0;
  write_a = 0;
  write_b = 0;
  write_vb = 0;
  reg_a = write<s>(write_a,o);
  reg_b = write<s>(write_b,o);
  reg_vb = write<s>(write_vb,o);
  write_addr = reg_b;
  read_addr = zero<s>();
  o = ram_module<s>(write_addr,read_addr,write_ram,reg_vb);
end where 

const s = 2

lt<n>(a:[n],b:[n]) = c where
  if n=0 then
    c=0
  else
    c=mux(a[n-1]^b[n-1],b[n-1],lt<n-1>(a[..n-2],b[..n-2]))
  end if
end where

eq<n>(a:[n],b:[n]) = c where
  if n=0 then
    c=1
  else
    c=not(a[0]^b[0])&eq<n-1>(a[1..],b[1..])
  end if
end where

main(a:[s],b:[s]) = (c,d) where
  c = lt<s>(a,b);
  d = eq<s>(a,b);
end where
